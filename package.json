    "vue-router": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.24.0",
    "file-loader": "^0.9.0",
